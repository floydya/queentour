.hot
	display block
	min-width 1110px
	background #FAFAFA 
	border-top 1px solid #EEEEEE
	border-bottom 1px solid #EEEEEE
	&__content 
		padding-bottom 35px
	&__part 
		width 100%
	&__block
		width 31%
		height 328px
		float left 
		margin-right 3.5%
		margin-bottom 20px 
		position relative 
	&__block:nth-child(3n) 
		margin-right 0
	&__block-link
		text-decoration none 
		color #fff
	&__picture
		width 100%
		position absolute 
		top 0 
		left 0
		z-index 1
		&--size 
			max-width 350px
			max-height 330px
			width 100%
	&__icon-one 
		position absolute 
		top 0 
		left 0
		z-index 2
	&__description
		width 100%
		padding 0 0 10px 16px
		background #000
		opacity 0.7
		position absolute 
		bottom 5px 
		left 0
		z-index 2
	&__desc-top
		margin-bottom 5px
	&__desc-title
		width 50%
		font-size 18px
		padding-top 17px
	&__icon-two
		width 140px
		position absolute 
		right 0 
		bottom 15px 
		z-index 3
		background url(../img/icon-2.png) no-repeat  
		padding 12px 3px 16px 19px
		font-size 22px 
		color #000
		&--span 
			font-size 14px
	&__desc-more 
		display none
		margin-top 15px
		font-size 16px
	&__desc-button
		width 35%
		margin 20px auto 0 
		text-align center
		padding-top 5px 
		padding-bottom 7px
		font-size 12px 
		text-transform uppercase 
		color #fff 
		border 1px solid #fff 
		border-radius 45px
	&__desc-button:hover, &__desc-button:active  
		background #FFC600
		color #000
	&__block-link:hover &__desc-more
		display block
	&__block-link:hover &__icon-two
		bottom 116px
	&__button
		display block
		width 15% 
		margin 20px auto 0
		padding 12px 0 
		color #343434
		font-weight 900
		border 1px solid #343434
		border-radius 45px
		text-transform uppercase
		text-align center
		text-decoration none
	&__button:hover 
		background #FFC600
		color #000
.news
	display block 
