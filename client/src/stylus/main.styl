			text-transform uppercase 
	&__ban-bot
		margin-top 7px 
		font-size 18px
	&__forma 
		width 86.5%
		margin-top 15px
	&__part-form 
		width 33%
		float left 
		margin-right 1%
	&__part-form:nth-child(2)
		margin-right 0
	&__part-form:last-child 
		margin-right 0
	&__input 
		display block 
		width 100%
		padding 12px 15px 
		font-size 16px
		background #fff 
		border none 
		color #161616
		border-radius 5px
	&__fon 
		background url(../img/back-inp.png) no-repeat
	&__inp-sub
		display block
		width 85%
		font-size 14px
		padding 12px 18px
		text-transform uppercase 
		text-align center
		background #FFC600
		border-radius 45px
		border none
		outline none
		-webkit-appearance none	
	&__inp-sub:hover 
		cursor pointer 
		background #ffd200
	&__help 
		display none
		padding-left 10px
		color #C82A1B
	&__all-news
		position absolute 
		right 0
		top 55px 
		text-transform uppercase
		color #424242
	&__all-news:hover
