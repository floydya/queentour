@import 'general.styl'
.header
	display block 
	min-width 1110px 
	background #343434
	&__content 
		width 100%
		padding 10px 
	&__logo
		width 10%
		float left
		&--size 
			max-width 100px
			max-height 117px 
	&__menu 
		width 53%
		float left 
		margin-left 12%
	&__nav
		width 100%
		margin-top 50px
	&__list 
		display table
		width 100%
		list-style none
	&__item 
		padding 0 10px
		text-align center
		display table-cell
	&__link 
		color #fff
		font-size 15px 
		text-decoration none
	&__link:hover 
		color #FCD45B 
		
	&__call
		width 17%
		float right
		margin-top 15px
		position relative
	&__number
		margin-bottom 12px
		color #fff
		&--tel 
			width 22px
			height 22px
			vertical-align middle
		&--tel:hover 
			cursor pointer
		&--strel
			padding-left 4px
			padding-right 9px
			vertical-align middle
		&--strel:hover 
			cursor pointer
		&--span 
			font-size 22px
			vertical-align middle
	&__button
		padding 12px 0 15px 0
		text-align center 
		font-weight bold 
		text-transform uppercase
		background #FFC600
		color #000
		border-radius 60px
	&__button:hover 
		background #FFD200
		cursor pointer
	&__telephone
		display none
		position absolute
		left 19px 
		top 30px
		background #fff 
		border 1px solid #D9D9D9
		opacity 0.9
	&__tel
		padding 10px 17px
		color #343434
		font-size 18px
		font-weight bold
		border-bottom 1px solid #DADADA
	&__tel:last-child 
		border-bottom none
	&__adaptive-icon, &__adaptive-icon--hover
		display none
	&__none 
		display block
.header__link-acive
	color #FCD45B
.navigation
	display block 
	min-width 1110px
	background #fff
	&__nav
		width 100%
	&__ul
		display table
		width 100%
		list-style none
		padding-right 28px
	&__li
		text-align center
		display table-cell
	&__a
		display block 
		color #4F4F4F
		text-decoration none 
		font-size 18px
		padding 17px 29px 20px 29px
	&__a:hover, &__a:active
		background #FFC600
.active-link
	background #FFC600

.slider 
	display block
	width 100%
	&__block
		width 45%
		position absolute
		top 325px
		left 60px
		z-index 1000
	&__text
		width 70% 
		float left 
		color #fff 
		font-size 62px
		line-height 0.9
	&__button
		display block 
		width 28% 
		float right
		color #fff 
		text-decoration none 
		padding 12px 0
		border 1px solid #fff 
		border-radius 50px
		text-transform uppercase
		text-align center
		margin-top 63px
	&__button:hover 
		background #FFC600
		color #000
	&__shadow
		position absolute
		bottom 2px 
		left 0
		height 100%
		width 100% 
		background url(../img/shadow.png) no-repeat 
		background-position bottom center
		z-index 1
	&__shadow-two
		position absolute
		bottom 0
		left 0
		height 100%
		width 100% 
		background url(../img/shadow.png) no-repeat 
		background-position bottom center
		z-index 10000
				
.rest
	display block 
	min-width 1110px
	background #FFFFFF
